    -- NPC Teleport
    Tabs.Teleport:AddDropdown("TeleportNPC", {
        Title = "Teleport to NPC",
        Description = "Select NPC to teleport to",
        Values = {
            "Alex - Sell Fish",
            "Billy Bob - Rod Skin",
            "Boat Expert - Boat Shop",
            "Joe - Rod Shop",
            "Scientist - Lucky Potion",
